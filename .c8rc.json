{
    "all": true,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "branches": 100,
    "extension": [".js"],
    "src": "target/source",
    "include": ["target/source/**/*"],
    "exclude": ["target/source/bin.js", "target/source/network.js"],
    "reporter": ["lcov", "text-summary", "json"],
    "check-coverage": true,
    "reportDir": "./target/coverage",
    "tempDirectory": "./target/c8_output"
}
